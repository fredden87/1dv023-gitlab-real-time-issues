<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Real-time issues</title>
    <link rel="stylesheet" href="/css/styles.css">
    <script type="module" src="/js/app.js"></script>
    <script src="/socket.io/socket.io.js"></script>
</head>

<body>
    <template id="issue-template">
            <div data-issue-id="" class="issue-card">
                <div id="issue-card-head"></div>
                <ul id="issue-card-ul">
                    <li id="author"></li>
                    <li id="description"></li>
                    <li id="url"></li>
                    <li id="comments"></li>
                    <li id="created-at"></li>
                    <li id="updated-at"></li>
                </ul>
            </div>
    </template>
    <div id="container">
        {{{body}}}
    </div>
</body>

</html>